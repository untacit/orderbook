<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity OrderedBook.
    -->
    <changeSet id="20191109023551-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="book_id"
                                 baseTableName="ordered_book"
                                 constraintName="fk_ordered_book_book_id"
                                 referencedColumnNames="id"
                                 referencedTableName="book"/>

        <addForeignKeyConstraint baseColumnNames="order_book_domain_id"
                                 baseTableName="ordered_book"
                                 constraintName="fk_ordered_book_order_book_domain_id"
                                 referencedColumnNames="id"
                                 referencedTableName="order_book_domain"/>

    </changeSet>
</databaseChangeLog>
